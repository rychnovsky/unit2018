{
  "name": "frontend-project-starter",
  "version": "1.0.0",
  "description": "Frontend Project Starter with basic setup",
  "main": "index.js",
  "repository": "git@github.com:strv-academy-2017/frontend-project-starter.git",
  "author": "dannytce <dankijkov@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run build:clean",
    "build": "webpack -p --config=internals/webpack/config.prod.js",
    "build:clean": "rimraf ./dist",
    "stage": "webpack-dev-server --config=internals/webpack/config.prod.js -p --open",
    "dev": "webpack-dev-server --config=internals/webpack/config.dev.js --open",
    "lint": "eslint src",
    "lint:autofix": "eslint --fix",
    "lint:css": "stylelint src/**/*.scss",
    "lint:staged": "lint-staged",
    "start": "watchify -o bundle.js -v -d src/index.js"
  },
  "lint-staged": {
    "*.js": "lint",
    "*.scss": "lint:css"
  },
  "pre-commit": "lint:staged",
  "devDependencies": {
    "@strv/eslint-config-javascript": "^7.1.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "browserify": "^14.5.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.2.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.29.0",
    "import-glob-loader": "^1.1.0",
    "lint-staged": "^4.0.2",
    "node-sass": "^4.5.3",
    "precommit": "^1.2.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "url-loader": "^0.5.9",
    "watchify": "^3.9.0",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1"
  },
  "dependencies": {
    "animate.css": "^3.6.1",
    "babel-polyfill": "^6.26.0",
    "js-cookie": "^2.2.0",
    "prop-types": "^15.5.10",
    "ramda": "^0.24.1",
    "react": "^15.6.1",
    "react-animate-on-scroll": "^2.1.0",
    "react-countup": "^3.0.3",
    "react-dom": "^15.6.1",
    "react-google-maps": "^9.4.5",
    "react-google-maps-loader": "^4.1.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-slick": "^0.20.0",
    "redux": "^3.7.2",
    "redux-persist": "^4.9.1",
    "redux-saga": "^0.15.6",
    "styled-components": "^2.1.2",
    "superagent": "^3.8.2",
    "whatwg-fetch": "^2.0.3"
  }
}
